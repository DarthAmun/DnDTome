{
  "name": "dndtome",
  "version": "0.1.3",
  "description": "Program to learn react and electron on the subject of pen and paper",
  "author": "DarthAmun",
  "homepage": "https://github.com/DarthAmun/dndtome",
  "repository": {
    "type": "git",
    "url": "https://github.com/DarthAmun/dndtome.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "npm run package-win && npm run package-linux",
    "package-win": "electron-packager ./ --out=./builds --app-version='0.1.3' --icon=./src/assets/img/dice_icon.ico --overwrite --platform=win32 --arch=x64 --asar",
    "package-linux": "electron-packager ./ --out=./builds --asar --icon=./src/assets/img/dice_icon.png --overwrite --platform=linux --arch=x64",
    "postinstall": "npm run appDeps-win && npm run appDeps-linux",
    "appDeps-win": "electron-builder install-app-deps --platform=win32 --arch=x64 ",
    "appDeps-linux": "electron-builder install-app-deps --platform=linux --arch=x64"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^3.2.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.28.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.1",
    "@fortawesome/free-regular-svg-icons": "^5.11.1",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "node-gyp": "^6.0.0",
    "react-particles-js": "^2.7.0",
    "recharts": "^1.8.5",
    "sqlite3": "^4.1.0"
  }
}
